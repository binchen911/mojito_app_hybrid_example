[
    {
        "settings": [ "master" ],

        "shareYUIInstance": true,

        "staticHandling": {
            "prefix": "mojito_app_hybrid_example",
            "appName": "yahoo.hybrid.app",
            "frameworkName": "yahoo.hybrid.mojtio"
        },
        "-log":{
            "client":{
                "level":" debug",
                "yui": true,
                "timestamp": false
            },
            "server":{
                "level": "debug",
                "yui": true,
                "timestamp": true
            }
        },
        "builds": {
            "html5app": {
                "urls": [
                    "/mojito_app_hybrid_example/tablet/index.html",
                    "/mojito_app_hybrid_example/phone/index.html"
                ]
            }
        }
    },
    {
        "settings": [ "environment:development" ],
        "staticHandling": {
            "forceUpdate": true
        }
    }
]
